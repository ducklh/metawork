# Docker Environment Variables
# Copy this file to .env và điền thông tin thực tế

# Google OAuth Credentials
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# MongoDB Credentials (phải khớp với docker-compose.yml)
MONGO_INITDB_ROOT_USERNAME=admin
MONGO_INITDB_ROOT_PASSWORD=password123

# Security (thay đổi trong production)
JWT_SECRET=your_jwt_secret_key_change_in_production_docker
SESSION_SECRET=your_session_secret_change_in_production_docker

# Application URLs
CLIENT_URL=http://localhost:3000
API_URL=http://localhost:5000/api